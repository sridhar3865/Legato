<apex:page StandardController="Case">
<apex:form >
<apex:pageblock >
<apex:pageBlockSection title="Case Details">
<apex:inputField value="{!Case.Priority}" />
<apex:inputField value="{!Case.Status}" />
<apex:inputField value="{!Case.PotentialLiability__c}"/>
<apex:inputField value="{!Case.Subject}"/>
<apex:inputField value="{!Case.Type}"/>
<apex:inputField value="{!Case.Reason}"/>

<apex:inputField value="{!Case.Product__c}"/>
<apex:inputField value="{!Case.SLAViolation__c}"/>
<apex:commandButton value="Save" action="{!Save}"/>
<apex:commandButton value="Cancel" action="{!Cancel}"/>
<apex:commandButton value="Creare" action="{!Create}"/>

</apex:pageBlockSection>
</apex:pageblock>
</apex:form>
 </apex:page>